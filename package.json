{
  "private": true,
  "type": "module",
  "scripts": {
    "generate:db-schema": "prisma generate && prisma db push && uv run scripts/db_into_schema.py temp.db",
    "generate:icon": "tauri icon public/icon.svg",
    "install:pre": "wasm-pack build wasm/endpoint --target web --release",
    "install:post": "bun run generate:icon && cargo run --bin generate_ipc_bindings && bun run generate:db-schema && playwright install --with-deps",
    "check": "tsc && eslint src",
    "dev": "vinxi dev",
    "build": "bun run check && vinxi build",
    "preview": "wrangler dev",
    "test": "playwright test",
    "dev:native": "vinxi dev --config native.config.ts --host 0.0.0.0",
    "build:native": "bun run check && vinxi build --config native.config.ts",
    "android:init": "rimraf native/gen/android && tauri android init && uv run scripts/insert_android_sign_config.py keystore.properties",
    "android:build": "tauri android build --apk --split-per-abi --target aarch64"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.10.0",
    "@types/bun": "^1.3.8",
    "daisyui": "^5.5.18",
    "eslint": "^10.0.0",
    "prisma": "^7.3.0",
    "prisma-kysely": "^3.0.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "typescript-eslint": "^8.54.0",
    "wrangler": "^4.63.0"
  },
  "dependencies": {
    "@pupu/endpoint": "file:wasm/endpoint/pkg",
    "@floating-ui/dom": "^1.7.5",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.4",
    "@solidjs/start": "^1.2.1",
    "@tanstack/solid-form": "^1.28.0",
    "@tanstack/solid-virtual": "^3.13.18",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-os": "^2.3.2",
    "arktype": "^2.1.29",
    "comlink": "^4.4.2",
    "kysely": "^0.28.11",
    "lucide-solid": "^0.563.0",
    "octokit": "^5.0.5",
    "opfs-worker": "^1.3.1",
    "radash": "^12.1.1",
    "solid-js": "^1.9.11",
    "tailwind-merge": "^3.4.0",
    "taurpc": "^1.8.1",
    "theme-change": "^2.5.0",
    "vinxi": "^0.5.11",
    "wa-sqlite": "github:rhashimoto/wa-sqlite#v1.0.9"
  }
}
