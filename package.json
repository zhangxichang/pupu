{
  "private": true,
  "type": "module",
  "stackblitz": {
    "installDependencies": false,
    "startCommand": false
  },
  "scripts": {
    "install:pre": "wasm-pack build wasm/endpoint --target web --release",
    "install:post": "tauri icon public/icon.svg && cargo run --bin generate_ipc_bindings && bun run generate:db-schema && bun run install:playwright-deps",
    "generate:db-schema": "prisma generate && prisma db push && uv run scripts/db_into_schema.py temp.db",
    "install:playwright-deps": "$env:PLAYWRIGHT_BROWSERS_PATH=0 && playwright install --with-deps",
    "check": "tsc && eslint src",
    "dev": "vinxi dev",
    "build": "bun run check && vinxi build",
    "preview": "wrangler dev",
    "test": "playwright test",
    "dev:native": "vinxi dev --config native.config.ts",
    "build:native": "bun run check && vinxi build --config native.config.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "prisma": "^7.2.0",
    "prisma-kysely": "^3.0.0",
    "tailwindcss": "^4.1.18",
    "typescript-eslint": "^8.53.1",
    "wrangler": "^4.59.2"
  },
  "dependencies": {
    "@dp2p/endpoint": "file:wasm/endpoint/pkg",
    "@floating-ui/dom": "^1.7.4",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.4",
    "@solidjs/start": "^1.2.1",
    "@tanstack/solid-form": "^1.27.7",
    "@tanstack/solid-virtual": "^3.13.18",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-os": "^2.3.2",
    "arktype": "^2.1.29",
    "comlink": "^4.4.2",
    "kysely": "^0.28.10",
    "lucide-solid": "^0.563.0",
    "octokit": "^5.0.5",
    "opfs-worker": "^1.3.1",
    "radash": "^12.1.1",
    "solid-js": "^1.9.10",
    "tailwind-merge": "^3.4.0",
    "taurpc": "^1.8.1",
    "theme-change": "^2.5.0",
    "vinxi": "^0.5.11",
    "wa-sqlite": "github:rhashimoto/wa-sqlite#v1.0.9"
  }
}
